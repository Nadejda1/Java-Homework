public class H2_7 {

	public static void main(String[] args) {

		char a = 'c';
		System.out.println(c);

		char a = 'a';
		System.out.println(a);

		if (a > c) {
			System.out.println("a > c");
		} else {
			System.out.println("a <= c");
		}

		System.out.println((int) a);
		System.out.println((int) c);
	}
}
