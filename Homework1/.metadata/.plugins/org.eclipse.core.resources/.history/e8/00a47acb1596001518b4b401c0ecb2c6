import java.util.Scanner;

public class H3_2 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in, "UTF-8");
		System.out.println("Pls insert N:");
		int N = input.nextInt();
		for (int i = 0; i <= N; i++) {
			if (N % 3 != 0 || N % 7 != 0) {
				System.out.print(i);
			}
			i++;
		}
	}
}
